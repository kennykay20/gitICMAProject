<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Home Page</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/alertify.core.css" rel="stylesheet" />
    <link href="~/Content/alertify.default.css" rel="stylesheet" />
    <link href="~/Content/fonts/css/all.css" rel="stylesheet" />
    <link href="~/Content/fonts/css/all.min.css" rel="stylesheet" />
    <link href="~/Content/fonts/css/fontawesome.css" rel="stylesheet" />
    <link href="~/Content/fonts/css/svg-with-js.min.css" rel="stylesheet" />
    <link href="~/Content/fonts/css/regular.min.css" rel="stylesheet" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("ICMA_TEST_DEMO", "Index", "User", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "User")</li>
                    <li>@Html.ActionLink("Course", "Index", "Course")</li>
                    <li>@Html.ActionLink("Attempt Score", "Index", "AttemptCourse")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        
    </div>
    <footer class="container">
        <p>&copy; @DateTime.Now.Year - My ASP.NET MVC Application</p>
    </footer>
    @*<script src="~/Scripts/jquery.min.js"></script>*@
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Content/alertify.min.js"></script>
    <script src="~/Content/fonts/js/all.js"></script>
    <script src="~/Content/fonts/js/all.min.js"></script>
    <script src="~/Content/fonts/js/regular.min.js"></script>
    <script src="~/Content/fonts/js/regular.js"></script>
    <script src="~/Content/fonts/js/fontawesome.js"></script>
    <script src="~/Content/fonts/js/fontawesome.min.js"></script>
    <script src="~/Scripts/scriptFile.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
